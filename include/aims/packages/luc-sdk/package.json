{
  "name": "@plugmein/luc-sdk",
  "version": "1.0.0",
  "description": "Layered Usage Calculator - Framework-agnostic usage tracking, quota gating, and cost estimation library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "import": "./dist/presets.mjs",
      "require": "./dist/presets.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "usage-tracking",
    "quota-management",
    "billing",
    "metering",
    "cost-estimation",
    "saas",
    "api-usage",
    "rate-limiting",
    "subscription",
    "usage-calculator",
    "quota-gating"
  ],
  "author": "PlugMeIn Cloud <dev@plugmein.cloud>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BoomerAng9/LUC-Locale-Universal-Calculator.git"
  },
  "homepage": "https://luc.plugmein.cloud",
  "bugs": {
    "url": "https://github.com/BoomerAng9/LUC-Locale-Universal-Calculator/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
