{
  "$schema": "https://openclaw.dev/schemas/config.json",
  "version": "2026.2.3",
  "instance": {
    "name": "A.I.M.S. Deploy Platform",
    "prefix": "[A.I.M.S.]",
    "agentBranding": "Boomer_Ang",
    "agentPluralBranding": "Boomer_Angs",
    "branding": {
      "enabled": true,
      "logo": "/assets/aims-logo.png",
      "theme": "dark"
    }
  },

  "deploy": {
    "description": "Deploy Platform - Container yard with cranes, crew, and traffic control",
    "metaphor": "Port Authority (internal) / Deploy (external)",

    "vocabulary": {
      "userFacing": {
        "deployIt": {
          "name": "Deploy It",
          "description": "Fast dispatch lane - manifest preview → confirm → shift runs",
          "type": "lane"
        },
        "guideMe": {
          "name": "Guide Me",
          "description": "Consultative lane - needs analysis → manifest generation → approval",
          "type": "lane"
        },
        "manifest": {
          "name": "Manifest",
          "description": "The deployment plan ACHEEVY generates (what's being moved, where, and why)"
        },
        "crew": {
          "name": "Crew",
          "description": "The Deploy team executing the manifest (OpenClaw + bots)"
        },
        "shift": {
          "name": "Shift",
          "description": "A single execution session with start/end, audit trail, and receipt"
        },
        "receipt": {
          "name": "Receipt",
          "description": "Proof of what ran + what changed + costs + checks (ties to LUC + KYB)"
        }
      },
      "internal": {
        "acheevy": "Orchestrator",
        "boomerAngs": "Supervisors",
        "openClaw": "Execution crew",
        "bots": "Atomic workers",
        "luc": "Locale Universal Calculator - metering + quotas + cost signals",
        "kyb": "Identity + audit trail"
      }
    },

    "visualMapping": {
      "containerStacks": "Plugs / tenant workloads - each container = deployable unit with ID, owner, plan, state",
      "craneArms": "OpenClaw (Deploy Crew) - mass movement + placement of workloads",
      "groundCrew": "Boomer_Angs (Supervisors + Specialists) - oversight, safety, verification, dispatch",
      "smallVehicles": "Spawned bots (Boomer_Ang-classed workers) - atomic tasks: move, check, patch, restart",
      "centralLanes": "Execution lanes - Deploy It and Guide Me",
      "sunrise": "Operational clarity - status lights, manifests, receipts, audit trails",
      "emblems": "KYB identity + trust marks - verified and governed badge"
    },

    "lanes": {
      "deployIt": {
        "type": "fast_dispatch",
        "flow": ["manifest_preview", "confirm", "shift_runs"],
        "questions": "few",
        "execution": "fast",
        "output": "receipt"
      },
      "guideMe": {
        "type": "consultative",
        "flow": ["needs_analysis", "goal", "constraints", "target_outcome", "deployment_shape", "manifest_approval"],
        "acheevy_collects": ["goal", "constraints", "target_outcome", "deployment_shape"],
        "output": "manifest",
        "then": "switches to Deploy It automatically"
      }
    },

    "registryPaths": {
      "capabilities": "/app/deploy-platform/registry/capability-registry.json",
      "botMonikers": "/app/deploy-platform/registry/bot-moniker-rules.json",
      "contracts": "/app/deploy-platform/contracts/shift-contract-schema.json"
    }
  },

  "governance": {
    "description": "Chain of command: ACHEEVY → Boomer_Ang → Contract → OpenClaw → Bots → ACHEEVY",
    "workflow": {
      "1_approval": {
        "actor": "ACHEEVY",
        "action": "approves_plan",
        "description": "ACHEEVY reviews and approves the execution plan",
        "required": true
      },
      "2_validation": {
        "actor": "Boomer_Ang",
        "action": "validates_scope",
        "description": "Boomer_Ang validates scope and enforces limits",
        "required": true
      },
      "3_contract": {
        "actor": "System",
        "action": "issues_contract",
        "description": "Formal contract issued to OpenClaw with exact specifications",
        "required": true
      },
      "4_execution": {
        "actor": "OpenClaw",
        "action": "executes_exactly",
        "description": "OpenClaw executes exactly what contract specifies - no more, no less",
        "required": true
      },
      "5_reporting": {
        "actor": "Chicken_Hawk",
        "action": "report_logs_only",
        "description": "Chicken_Hawks report execution logs only - small but relentless workers, no decisions, no deviations",
        "required": true
      },
      "6_closure": {
        "actor": "ACHEEVY",
        "action": "verifies_and_closes",
        "description": "ACHEEVY verifies completion and closes the task",
        "required": true
      }
    },
    "roles": {
      "ACHEEVY": {
        "type": "orchestrator",
        "permissions": ["approve", "reject", "verify", "close"],
        "canInitiate": true,
        "canTerminate": true,
        "description": "AI orchestrator - approves plans and verifies completion"
      },
      "Boomer_Ang": {
        "type": "validator",
        "permissions": ["validate", "limit", "scope"],
        "canInitiate": false,
        "canTerminate": false,
        "description": "Scope validator - enforces boundaries and limits"
      },
      "OpenClaw": {
        "type": "executor",
        "permissions": ["execute"],
        "canInitiate": false,
        "canTerminate": false,
        "description": "Execution engine - follows contracts exactly"
      },
      "Chicken_Hawk": {
        "type": "worker",
        "permissions": ["log"],
        "canInitiate": false,
        "canTerminate": false,
        "description": "Small but relentless workers - execute tasks and report logs only, can power up when needed",
        "evolution": {
          "base": "Chick - atomic tasks",
          "powered": "Hawk - parallel tasks",
          "maxPower": "Super_Hawk - complex operations"
        }
      }
    },
    "contracts": {
      "format": "json",
      "requiredFields": ["taskId", "scope", "limits", "expectedOutputs", "approvedBy"],
      "immutable": true,
      "signatureRequired": true
    },
    "enforcement": {
      "strictMode": true,
      "noDeviations": true,
      "logAllActions": true,
      "requireApprovalChain": true
    }
  },
  "gateway": {
    "port": 8001,
    "host": "0.0.0.0",
    "cors": {
      "enabled": true,
      "origins": ["http://localhost:3000", "http://aims-frontend:3000"]
    }
  },
  "dashboard": {
    "enabled": true,
    "port": 8002,
    "auth": {
      "required": true,
      "method": "bearer"
    }
  },

  "deliverables": {
    "branding": {
      "enabled": true,
      "prefix": "[A.I.M.S.]",
      "agentName": "Boomer_Ang",
      "signature": "Generated by A.I.M.S. Boomer_Ang - AI MANAGED SOLUTIONS",
      "watermark": {
        "enabled": true,
        "text": "A.I.M.S.",
        "position": "footer"
      },
      "metadata": {
        "includeTimestamp": true,
        "includeModel": false,
        "includeAgentId": true,
        "agentIdPrefix": "Boomer_Ang_",
        "customFields": {
          "platform": "A.I.M.S.",
          "vendor": "AI MANAGED SOLUTIONS",
          "agentType": "Boomer_Ang"
        }
      }
    },
    "formats": {
      "code": {
        "headerComment": "// Generated by A.I.M.S. Boomer_Ang - AI MANAGED SOLUTIONS",
        "includeHeader": true
      },
      "document": {
        "footer": "Powered by A.I.M.S. Boomer_Angs",
        "includeFooter": true
      },
      "report": {
        "coverPage": true,
        "brandingHeader": "A.I.M.S. Boomer_Ang Analysis Report",
        "disclaimer": "This report was generated by an A.I.M.S. Boomer_Ang - AI MANAGED SOLUTIONS automated analysis."
      }
    }
  },

  "taskModeling": {
    "behavior": {
      "approach": "contract-bound",
      "verbosity": "logs-only",
      "confirmationRequired": false,
      "autoRetry": false,
      "maxRetries": 0,
      "deviation": "prohibited",
      "autonomy": "none"
    },
    "planning": {
      "enabled": false,
      "description": "Planning is done by ACHEEVY, not bots",
      "botsCanPlan": false,
      "requiresApproval": true
    },
    "execution": {
      "mode": "contract-exact",
      "parallelTasks": false,
      "maxConcurrent": 1,
      "checkpointInterval": 1,
      "rollbackOnFailure": true,
      "haltOnDeviation": true,
      "requireContractMatch": true
    },
    "communication": {
      "style": "logs-only",
      "updateFrequency": "every-action",
      "errorReporting": "immediate",
      "successAcknowledgment": "log-entry",
      "noDecisionMaking": true
    },
    "qualityControl": {
      "selfReview": false,
      "validateOutputs": true,
      "verifiedBy": "ACHEEVY",
      "botsCannotApprove": true
    }
  },

  "whiteLabel": {
    "enabled": false,
    "reseller": {
      "name": "",
      "logo": "",
      "website": "",
      "supportEmail": ""
    },
    "branding": {
      "platformName": "A.I.M.S.",
      "tagline": "AI MANAGED SOLUTIONS",
      "primaryColor": "#F59E0B",
      "accentColor": "#FCD34D",
      "fontFamily": "Permanent Marker"
    },
    "customization": {
      "hideOpenClawBranding": false,
      "customResponsePrefix": "[A.I.M.S.]",
      "customWatermark": "A.I.M.S.",
      "customSignature": "Powered by A.I.M.S. Boomer_Angs - AI MANAGED SOLUTIONS",
      "customDomain": "",
      "agentTerminology": {
        "singular": "Boomer_Ang",
        "plural": "Boomer_Angs"
      }
    },
    "features": {
      "allowModelSelection": true,
      "allowCustomAgents": true,
      "allowCustomBoomerAngs": true,
      "allowAPIAccess": true,
      "maxUsers": -1,
      "maxBoomerAngs": -1
    },
    "billing": {
      "passthrough": true,
      "markupPercentage": 0,
      "customPricing": false
    }
  },

  "models": {
    "provider": "openrouter",
    "default": "moonshot/kimi-k2.5",
    "fallback": "anthropic/claude-sonnet-4.6",
    "available": [
      {
        "id": "moonshot/kimi-k2.5",
        "name": "KIMI K2.5",
        "description": "Strong reasoning, cost-effective",
        "capabilities": ["chat", "code", "analysis"]
      },
      {
        "id": "anthropic/claude-sonnet-4.6",
        "name": "Claude Sonnet 4.6",
        "description": "High-quality balanced model",
        "capabilities": ["chat", "code", "analysis", "vision"]
      },
      {
        "id": "anthropic/claude-opus-4.5",
        "name": "Claude Opus 4.5",
        "description": "Most capable reasoning",
        "capabilities": ["chat", "code", "analysis", "vision", "complex-reasoning"]
      },
      {
        "id": "openai/gpt-5.2",
        "name": "GPT-5.2",
        "description": "Latest OpenAI model",
        "capabilities": ["chat", "code", "analysis", "vision"]
      },
      {
        "id": "deepseek/deepseek-r1",
        "name": "DeepSeek R1",
        "description": "Free reasoning model",
        "capabilities": ["chat", "code", "reasoning"]
      },
      {
        "id": "google/gemini-3-pro",
        "name": "Gemini 3 Pro",
        "description": "Google's latest model",
        "capabilities": ["chat", "code", "analysis", "vision"]
      }
    ]
  },
  "sandbox": {
    "mode": "docker",
    "scope": "agent",
    "image": "openclaw-sandbox:bookworm-slim",
    "resources": {
      "memory": "2G",
      "cpus": "1.0",
      "timeout": 300000
    },
    "security": {
      "networkAccess": false,
      "fileSystemReadOnly": true,
      "allowedPaths": ["/workspace", "/tmp"],
      "blockedCommands": ["sudo", "su", "chroot", "nc", "netcat"]
    }
  },
  "agents": {
    "terminology": {
      "singular": "Boomer_Ang",
      "plural": "Boomer_Angs",
      "description": "A.I.M.S. AI-powered autonomous agents for the Garage to Global seller journey"
    },
    "personas": {
      "enabled": true,
      "configPath": "/app/registry/personas.json",
      "available": [
        "Researcher_Ang",
        "Optimizer_Ang",
        "Syncer_Ang",
        "Responder_Ang",
        "Advertiser_Ang",
        "Creator_Ang",
        "Analyst_Ang",
        "Expander_Ang",
        "Builder_Ang",
        "Orchestrator_Ang"
      ],
      "defaultPersona": "Orchestrator_Ang"
    },
    "defaults": {
      "namePrefix": "Boomer_Ang_",
      "sandboxMode": "docker",
      "sandboxScope": "agent",
      "tools": {
        "browser": false,
        "camera": false,
        "elevated": false,
        "filesystem": true,
        "network": false
      },
      "limits": {
        "maxTokens": 8192,
        "maxTurns": 50,
        "timeout": 300000
      }
    },
    "registry": {
      "path": "/home/node/.openclaw/boomerangs",
      "personasPath": "/app/registry/personas.json",
      "autoDiscovery": true
    }
  },
  "security": {
    "dmPolicy": "pairing",
    "rateLimiting": {
      "enabled": true,
      "requests": 100,
      "windowMs": 60000
    },
    "blockedOperations": [
      "payment",
      "transfer",
      "purchase",
      "checkout",
      "billing",
      "credit_card",
      "stripe",
      "paypal"
    ]
  },
  "logging": {
    "level": "info",
    "format": "json",
    "includeTimestamp": true,
    "sanitize": true,
    "chickenHawkLogs": {
      "required": true,
      "captureAll": true,
      "fields": ["action", "input", "output", "contractRef", "timestamp", "powerLevel"],
      "noDecisions": true,
      "reportTo": "ACHEEVY"
    },
    "auditTrail": {
      "enabled": true,
      "immutable": true,
      "includeApprovalChain": true,
      "includeContractHash": true
    }
  },
  "integrations": {
    "cloudflareAIGateway": {
      "enabled": false,
      "url": "",
      "token": ""
    },
    "feishuLark": {
      "enabled": false,
      "appId": "",
      "appSecret": ""
    }
  }
}
