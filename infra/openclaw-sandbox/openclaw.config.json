{
  "$schema": "https://openclaw.dev/schemas/config.json",
  "version": "2026.2.3",
  "instance": {
    "name": "A.I.M.S. Agent Platform",
    "prefix": "[A.I.M.S.]",
    "branding": {
      "enabled": true,
      "logo": "/assets/aims-logo.png",
      "theme": "dark"
    }
  },
  "gateway": {
    "port": 8001,
    "host": "0.0.0.0",
    "cors": {
      "enabled": true,
      "origins": ["http://localhost:3000", "http://aims-frontend:3000"]
    }
  },
  "dashboard": {
    "enabled": true,
    "port": 8002,
    "auth": {
      "required": true,
      "method": "bearer"
    }
  },
  "models": {
    "provider": "openrouter",
    "default": "moonshot/kimi-k2.5",
    "fallback": "anthropic/claude-sonnet-4.6",
    "available": [
      {
        "id": "moonshot/kimi-k2.5",
        "name": "KIMI K2.5",
        "description": "Strong reasoning, cost-effective",
        "capabilities": ["chat", "code", "analysis"]
      },
      {
        "id": "anthropic/claude-sonnet-4.6",
        "name": "Claude Sonnet 4.6",
        "description": "High-quality balanced model",
        "capabilities": ["chat", "code", "analysis", "vision"]
      },
      {
        "id": "anthropic/claude-opus-4.5",
        "name": "Claude Opus 4.5",
        "description": "Most capable reasoning",
        "capabilities": ["chat", "code", "analysis", "vision", "complex-reasoning"]
      },
      {
        "id": "openai/gpt-5.2",
        "name": "GPT-5.2",
        "description": "Latest OpenAI model",
        "capabilities": ["chat", "code", "analysis", "vision"]
      },
      {
        "id": "deepseek/deepseek-r1",
        "name": "DeepSeek R1",
        "description": "Free reasoning model",
        "capabilities": ["chat", "code", "reasoning"]
      },
      {
        "id": "google/gemini-3-pro",
        "name": "Gemini 3 Pro",
        "description": "Google's latest model",
        "capabilities": ["chat", "code", "analysis", "vision"]
      }
    ]
  },
  "sandbox": {
    "mode": "docker",
    "scope": "agent",
    "image": "openclaw-sandbox:bookworm-slim",
    "resources": {
      "memory": "2G",
      "cpus": "1.0",
      "timeout": 300000
    },
    "security": {
      "networkAccess": false,
      "fileSystemReadOnly": true,
      "allowedPaths": ["/workspace", "/tmp"],
      "blockedCommands": ["sudo", "su", "chroot", "nc", "netcat"]
    }
  },
  "agents": {
    "defaults": {
      "sandboxMode": "docker",
      "sandboxScope": "agent",
      "tools": {
        "browser": false,
        "camera": false,
        "elevated": false,
        "filesystem": true,
        "network": false
      },
      "limits": {
        "maxTokens": 8192,
        "maxTurns": 50,
        "timeout": 300000
      }
    },
    "registry": {
      "path": "/home/node/.openclaw/agents",
      "autoDiscovery": true
    }
  },
  "security": {
    "dmPolicy": "pairing",
    "rateLimiting": {
      "enabled": true,
      "requests": 100,
      "windowMs": 60000
    },
    "blockedOperations": [
      "payment",
      "transfer",
      "purchase",
      "checkout",
      "billing",
      "credit_card",
      "stripe",
      "paypal"
    ]
  },
  "logging": {
    "level": "warn",
    "format": "json",
    "includeTimestamp": true,
    "sanitize": true
  },
  "integrations": {
    "cloudflareAIGateway": {
      "enabled": false,
      "url": "",
      "token": ""
    },
    "feishuLark": {
      "enabled": false,
      "appId": "",
      "appSecret": ""
    }
  }
}
