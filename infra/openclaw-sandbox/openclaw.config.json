{
  "$schema": "https://openclaw.dev/schemas/config.json",
  "version": "2026.2.3",
  "instance": {
    "name": "A.I.M.S. Boomer_Ang Platform",
    "prefix": "[A.I.M.S.]",
    "agentBranding": "Boomer_Ang",
    "agentPluralBranding": "Boomer_Angs",
    "branding": {
      "enabled": true,
      "logo": "/assets/aims-logo.png",
      "theme": "dark"
    }
  },
  "gateway": {
    "port": 8001,
    "host": "0.0.0.0",
    "cors": {
      "enabled": true,
      "origins": ["http://localhost:3000", "http://aims-frontend:3000"]
    }
  },
  "dashboard": {
    "enabled": true,
    "port": 8002,
    "auth": {
      "required": true,
      "method": "bearer"
    }
  },

  "deliverables": {
    "branding": {
      "enabled": true,
      "prefix": "[A.I.M.S.]",
      "agentName": "Boomer_Ang",
      "signature": "Generated by A.I.M.S. Boomer_Ang - AI MANAGED SOLUTIONS",
      "watermark": {
        "enabled": true,
        "text": "A.I.M.S.",
        "position": "footer"
      },
      "metadata": {
        "includeTimestamp": true,
        "includeModel": false,
        "includeAgentId": true,
        "agentIdPrefix": "Boomer_Ang_",
        "customFields": {
          "platform": "A.I.M.S.",
          "vendor": "AI MANAGED SOLUTIONS",
          "agentType": "Boomer_Ang"
        }
      }
    },
    "formats": {
      "code": {
        "headerComment": "// Generated by A.I.M.S. Boomer_Ang - AI MANAGED SOLUTIONS",
        "includeHeader": true
      },
      "document": {
        "footer": "Powered by A.I.M.S. Boomer_Angs",
        "includeFooter": true
      },
      "report": {
        "coverPage": true,
        "brandingHeader": "A.I.M.S. Boomer_Ang Analysis Report",
        "disclaimer": "This report was generated by an A.I.M.S. Boomer_Ang - AI MANAGED SOLUTIONS automated analysis."
      }
    }
  },

  "taskModeling": {
    "behavior": {
      "approach": "methodical",
      "verbosity": "concise",
      "confirmationRequired": false,
      "autoRetry": true,
      "maxRetries": 3
    },
    "planning": {
      "enabled": true,
      "breakdownThreshold": 3,
      "showProgress": true,
      "estimateEffort": false
    },
    "execution": {
      "parallelTasks": true,
      "maxConcurrent": 3,
      "checkpointInterval": 5,
      "rollbackOnFailure": true
    },
    "communication": {
      "style": "professional",
      "updateFrequency": "on-milestone",
      "errorReporting": "detailed",
      "successAcknowledgment": "brief"
    },
    "qualityControl": {
      "selfReview": true,
      "validateOutputs": true,
      "codeStyle": "consistent",
      "documentationLevel": "minimal"
    }
  },

  "whiteLabel": {
    "enabled": false,
    "reseller": {
      "name": "",
      "logo": "",
      "website": "",
      "supportEmail": ""
    },
    "branding": {
      "platformName": "A.I.M.S.",
      "tagline": "AI MANAGED SOLUTIONS",
      "primaryColor": "#F59E0B",
      "accentColor": "#FCD34D",
      "fontFamily": "Permanent Marker"
    },
    "customization": {
      "hideOpenClawBranding": false,
      "customResponsePrefix": "[A.I.M.S.]",
      "customWatermark": "A.I.M.S.",
      "customSignature": "Powered by A.I.M.S. Boomer_Angs - AI MANAGED SOLUTIONS",
      "customDomain": "",
      "agentTerminology": {
        "singular": "Boomer_Ang",
        "plural": "Boomer_Angs"
      }
    },
    "features": {
      "allowModelSelection": true,
      "allowCustomAgents": true,
      "allowCustomBoomerAngs": true,
      "allowAPIAccess": true,
      "maxUsers": -1,
      "maxBoomerAngs": -1
    },
    "billing": {
      "passthrough": true,
      "markupPercentage": 0,
      "customPricing": false
    }
  },

  "models": {
    "provider": "openrouter",
    "default": "moonshot/kimi-k2.5",
    "fallback": "anthropic/claude-sonnet-4.6",
    "available": [
      {
        "id": "moonshot/kimi-k2.5",
        "name": "KIMI K2.5",
        "description": "Strong reasoning, cost-effective",
        "capabilities": ["chat", "code", "analysis"]
      },
      {
        "id": "anthropic/claude-sonnet-4.6",
        "name": "Claude Sonnet 4.6",
        "description": "High-quality balanced model",
        "capabilities": ["chat", "code", "analysis", "vision"]
      },
      {
        "id": "anthropic/claude-opus-4.5",
        "name": "Claude Opus 4.5",
        "description": "Most capable reasoning",
        "capabilities": ["chat", "code", "analysis", "vision", "complex-reasoning"]
      },
      {
        "id": "openai/gpt-5.2",
        "name": "GPT-5.2",
        "description": "Latest OpenAI model",
        "capabilities": ["chat", "code", "analysis", "vision"]
      },
      {
        "id": "deepseek/deepseek-r1",
        "name": "DeepSeek R1",
        "description": "Free reasoning model",
        "capabilities": ["chat", "code", "reasoning"]
      },
      {
        "id": "google/gemini-3-pro",
        "name": "Gemini 3 Pro",
        "description": "Google's latest model",
        "capabilities": ["chat", "code", "analysis", "vision"]
      }
    ]
  },
  "sandbox": {
    "mode": "docker",
    "scope": "agent",
    "image": "openclaw-sandbox:bookworm-slim",
    "resources": {
      "memory": "2G",
      "cpus": "1.0",
      "timeout": 300000
    },
    "security": {
      "networkAccess": false,
      "fileSystemReadOnly": true,
      "allowedPaths": ["/workspace", "/tmp"],
      "blockedCommands": ["sudo", "su", "chroot", "nc", "netcat"]
    }
  },
  "agents": {
    "terminology": {
      "singular": "Boomer_Ang",
      "plural": "Boomer_Angs",
      "description": "A.I.M.S. AI-powered autonomous agents for the Garage to Global seller journey"
    },
    "personas": {
      "enabled": true,
      "configPath": "/app/registry/personas.json",
      "available": [
        "Researcher_Ang",
        "Optimizer_Ang",
        "Syncer_Ang",
        "Responder_Ang",
        "Advertiser_Ang",
        "Creator_Ang",
        "Analyst_Ang",
        "Expander_Ang",
        "Builder_Ang",
        "Orchestrator_Ang"
      ],
      "defaultPersona": "Orchestrator_Ang"
    },
    "defaults": {
      "namePrefix": "Boomer_Ang_",
      "sandboxMode": "docker",
      "sandboxScope": "agent",
      "tools": {
        "browser": false,
        "camera": false,
        "elevated": false,
        "filesystem": true,
        "network": false
      },
      "limits": {
        "maxTokens": 8192,
        "maxTurns": 50,
        "timeout": 300000
      }
    },
    "registry": {
      "path": "/home/node/.openclaw/boomerangs",
      "personasPath": "/app/registry/personas.json",
      "autoDiscovery": true
    }
  },
  "security": {
    "dmPolicy": "pairing",
    "rateLimiting": {
      "enabled": true,
      "requests": 100,
      "windowMs": 60000
    },
    "blockedOperations": [
      "payment",
      "transfer",
      "purchase",
      "checkout",
      "billing",
      "credit_card",
      "stripe",
      "paypal"
    ]
  },
  "logging": {
    "level": "warn",
    "format": "json",
    "includeTimestamp": true,
    "sanitize": true
  },
  "integrations": {
    "cloudflareAIGateway": {
      "enabled": false,
      "url": "",
      "token": ""
    },
    "feishuLark": {
      "enabled": false,
      "appId": "",
      "appSecret": ""
    }
  }
}
